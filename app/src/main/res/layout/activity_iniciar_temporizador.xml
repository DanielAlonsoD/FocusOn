<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/layoutIniciarTemporizador"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IniciarTemporizadorActivity">

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/encabezadoIniciarTemporizador"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/scampi"
        app:title="@string/textoTemporizador"
        app:titleTextColor="@color/gray_nurse"
        app:titleTextAppearance="@style/Titles"
        app:titleCentered="true"
        app:navigationIcon="@drawable/arrow_back"
        app:navigationIconTint="@color/gray_nurse"/>

</RelativeLayout>